<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.posmanagement.database.mapping.mccMapper">
    <select id="getMCC" parameterType="string" resultMap="MCCResult">
        SELECT * FROM mcctb WHERE uuid=#{uuid}
    </select>
    <resultMap type="com.posmanagement.database.MCCTable" id="MCCResult">
        <id property="uuid" column="uuid" jdbcType="VARCHAR" />
        <result property="mcc" column="mcc" jdbcType="VARCHAR" />
        <result property="status" column="status" jdbcType="VARCHAR"/>
    </resultMap>
</mapper>